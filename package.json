{
  "name": "@trungpham.liam/auxios",
  "version": "1.2.6",
  "description": "Production-ready TypeScript authentication library with automatic token refresh, multi-tab sync, and race condition prevention",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trungpham-liam/auxios.git"
  },
  "bugs": {
    "url": "https://github.com/trungpham-liam/auxios/issues"
  },
  "homepage": "https://github.com/trungpham-liam/auxios#readme",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./core": {
      "types": "./dist/core.d.ts",
      "import": "./dist/core.js",
      "require": "./dist/core.cjs"
    },
    "./storage": {
      "types": "./dist/storage.d.ts",
      "import": "./dist/storage.js",
      "require": "./dist/storage.cjs"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.js",
      "require": "./dist/react.cjs"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.js",
      "require": "./dist/utils.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:rolldown": "tsc && npx rolldown-vite build",
    "preview": "vite preview",
    "lint": "oxlint src && biome check src",
    "lint:fix": "biome check --write src",
    "format": "biome format --write src",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "auth",
    "authentication",
    "token",
    "jwt",
    "refresh-token",
    "axios",
    "fetch",
    "typescript",
    "expires-in",
    "token-refresh",
    "multi-tab-sync",
    "race-condition",
    "oauth2",
    "token-management",
    "auto-refresh"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.10.5",
    "@types/react": "^19.2.2",
    "oxlint": "^0.12.0",
    "rolldown-vite": "^7.1.20",
    "typescript": "^5.7.2",
    "vite": "^7.1.12",
    "vite-plugin-dts": "^4.5.4"
  },
  "peerDependencies": {
    "axios": "^1.0.0"
  },
  "peerDependenciesMeta": {
    "axios": {
      "optional": true
    }
  }
}
